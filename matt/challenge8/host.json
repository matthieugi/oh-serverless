{
  "version": "2.0",
  "extensions": {
    "eventHubs": {
      "eventProcessorOptions": {
          "maxBatchSize": 64,
          "prefetchCount": 256
        }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }
}